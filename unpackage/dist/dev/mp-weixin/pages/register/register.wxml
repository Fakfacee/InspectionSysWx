<view class="container"><view class="text-box"><text>请填写以下信息以完成注册</text><text>人员类别</text><radio-group data-event-opts="{{[['change',[['classChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{className}}" wx:for-item="item" wx:for-index="index" wx:key="value"><label class="uni-list-cell"><view><radio value="{{item.value}}" checked="{{index===current}}"></radio></view><view>{{item.name}}</view></label></block></radio-group><view class="graydivide"></view><view class="blanke"></view><text>所属公司</text><input class="inputText" placeholder="CFHI" data-event-opts="{{[['input',[['__set_model',['','subcontractor','$event',[]]]]]]}}" value="{{subcontractor}}" bindinput="__e"/><view class="blanke"></view><text>焊工号或其他标识ID</text><input class="inputText" placeholder="CFHI0001" data-event-opts="{{[['input',[['__set_model',['','identity','$event',[]]]]]]}}" value="{{identity}}" bindinput="__e"/><view class="blanke"></view><text>手机号</text><input data-event-opts="{{[['input',[['__set_model',['','phoneNumber','$event',[]]]]]]}}" class="inputText" value="{{phoneNumber}}" bindinput="__e"/><view class="blanke"></view><text>密码</text><input data-event-opts="{{[['input',[['__set_model',['','passWord','$event',[]]]]]]}}" class="inputText" value="{{passWord}}" bindinput="__e"/><view class="button-box"><button data-event-opts="{{[['tap',[['submitFun',['$event']]]]]}}" class="buttonMain" bindtap="__e">提交</button></view></view></view>